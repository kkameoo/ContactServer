<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Vo 객체 별칭 설정 -->
	<typeAliases>
		<typeAlias alias="userItem"
			type="himedia.contactserver.repository.vo.UserVo" />
		<typeAlias alias="memoItem"
			type="himedia.contactserver.repository.vo.MemoVo" />
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/memo.xml" />
	</mappers>
</configuration>